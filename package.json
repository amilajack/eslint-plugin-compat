{
  "name": "eslint-plugin-compat",
  "version": "1.0.2-0",
  "description": "Lint browser compatibility of API used",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amilajack/eslint-plugin-compat.git"
  },
  "keywords": [
    "eslint",
    "browser",
    "support",
    "api",
    "lint",
    "caniuse",
    "kangax"
  ],
  "author": "Amila Welihinda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amilajack/eslint-plugin-compat/issues"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "homepage": "https://github.com/amilajack/eslint-plugin-compat#readme",
  "scripts": {
    "build": "cross-env NODE_ENV=production rm -rf lib && babel src --out-dir lib",
    "flow": "flow check",
    "flow-typed": "flow-typed install",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "spec": "jest",
    "test": "cross-env NODE_ENV=test npm run lint && npm run flow && npm run spec && npm run build",
    "version": "npm run build"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "cross-env": "^3.1.4",
    "download-cli": "^1.0.1",
    "eslint": "^3.14.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-flowtype-errors": "^2.0.3",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "flow-bin": "^0.38.0",
    "flow-typed": "^2.0.0",
    "jest-cli": "^18.1.0",
    "tcomb": "^3.2.16"
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "browserslist": "^1.7.0",
    "caniuse-db": "1.0.30000618",
    "requireindex": "^1.1.0"
  },
  "peerDependencies": {
    "eslint": "^3.0.0"
  },
  "engines": {
    "node": ">=4.x",
    "npm": ">=3.x"
  },
  "devEngines": {
    "node": ">=4.x",
    "npm": ">=3.x"
  }
}
