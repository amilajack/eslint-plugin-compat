{
  "name": "eslint-plugin-compat",
  "version": "2.4.0",
  "description": "Lint browser compatibility of API used",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amilajack/eslint-plugin-compat.git"
  },
  "keywords": [
    "eslint",
    "browser",
    "support",
    "api",
    "lint",
    "caniuse",
    "kangax"
  ],
  "author": "Amila Welihinda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amilajack/eslint-plugin-compat/issues"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "homepage": "https://github.com/amilajack/eslint-plugin-compat#readme",
  "scripts": {
    "build": "cross-env NODE_ENV=production rm -rf lib && babel src --out-dir lib",
    "flow": "flow",
    "flow-typed": "flow-typed install --ignoreDeps peer dev",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "spec": "jest",
    "test": "cross-env NODE_ENV=test npm run spec && npm run build",
    "test-all": "cross-env NODE_ENV=test npm run lint && npm run flow && npm run spec && npm run build",
    "version": "npm run build"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.6",
    "download-cli": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-bliss": "^2.0.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "flow-bin": "^0.73.0",
    "flow-typed": "^2.4.0",
    "jest-cli": "^23.0.1"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "browserslist": "^3.2.8",
    "caniuse-db": "^1.0.30000846",
    "mdn-browser-compat-data": "^0.0.36",
    "requireindex": "^1.2.0"
  },
  "peerDependencies": {
    "eslint": "^3.0.0 || ^4.0.0"
  },
  "engines": {
    "node": ">=4.x",
    "npm": ">=3.x"
  },
  "devEngines": {
    "node": ">=4.x",
    "npm": ">=3.x"
  },
  "greenkeeper": {
    "ignore": [
      "caniuse-db"
    ]
  }
}
