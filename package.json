{
  "name": "eslint-plugin-compat",
  "version": "3.6.0",
  "description": "Lint browser compatibility of API used",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amilajack/eslint-plugin-compat.git"
  },
  "keywords": [
    "eslint",
    "browser",
    "support",
    "api",
    "lint",
    "caniuse",
    "kangax"
  ],
  "author": "Amila Welihinda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amilajack/eslint-plugin-compat/issues"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/amilajack/eslint-plugin-compat#readme",
  "scripts": {
    "build": "cross-env NODE_ENV=production rm -rf lib && babel src --out-dir lib --source-maps inline",
    "flow": "flow",
    "flow-typed": "flow-typed install --ignoreDeps peer dev",
    "lint": "eslint --ignore-path .gitignore .",
    "spec": "jest",
    "test": "yarn lint && yarn build && yarn spec",
    "version": "yarn build"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-flow": "^7.9.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-bliss": "^4.8.1",
    "eslint-plugin-prettier": "^3.1.3",
    "flow-bin": "^0.123.0",
    "flow-typed": "^3.1.0",
    "jest": "^25.5.3"
  },
  "dependencies": {
    "ast-metadata-inferer": "^0.1.1",
    "browserslist": "^4.11.1",
    "caniuse-db": "^1.0.30001040",
    "core-js": "^3.6.5",
    "lodash.memoize": "4.1.2",
    "mdn-browser-compat-data": "^1.0.17",
    "semver": "7.2.2"
  },
  "peerDependencies": {
    "eslint": "^3.0.0 || ^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "engines": {
    "node": ">=8.x"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/eslint-plugin-compat",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  },
  "browserslist": [
    "last 1 version"
  ],
  "eslintConfig": {
    "extends": "bliss",
    "env": {
      "node": true,
      "jest": true
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3,
          "targets": {
            "node": 10
          }
        }
      ],
      "@babel/preset-flow"
    ]
  },
  "renovate": {
    "extends": [
      "bliss"
    ],
    "ignoreDeps": [
      "caniuse-db",
      "mdn-browser-compat-data"
    ]
  }
}
